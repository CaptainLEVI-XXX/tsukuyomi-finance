{
  "name": "defi-ai-agent",
  "version": "1.0.0",
  "description": "Autonomous DeFi Investment AI Agent with ElizaOS",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm src/main.ts",
    "build": "tsc",
    "dev": "ts-node --esm --watch src/main.ts",
    "analyze": "ts-node --esm src/main.ts --analyze",
    "execute": "ts-node --esm src/main.ts --execute",
    "monitor": "ts-node --esm src/main.ts --monitor"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.55.0",
    "@elizaos/core": "^1.0.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
